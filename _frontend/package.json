{
  "name": "@kps3/boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "yarn theme:dev",
    "build": "yarn bundle:build && yarn templates:build",
    "theme:dev": "concurrently \"yarn templates:watch\" \"yarn bundle:dev\"",
    "component": "./create-component.sh",
    "bundle:dev": "cross-env NODE_ENV=development webpack-dev-server --mode=development --config webpack.config.js",
    "bundle:build": "cross-env NODE_ENV=production webpack --mode=production --config webpack.config.js",
    "templates:watch": "hbs-template-builder --path=./src/templates --output=./dist --watch",
    "templates:build": "hbs-template-builder --path=./src/templates --output=./dist"
  },
  "browserslist": [
    "> 2%",
    "last 2 versions",
    "ie 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@kps3/hbs-template-builder": "^1.0.4",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.3.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "file-loader": "^5.0.2",
    "import-glob": "^1.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^2.3.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "concurrently": "^5.0.1",
    "core-js": "3.5.0",
    "include-media": "^1.4.9",
    "poly-fluid-sizing": "jrutheiser/poly-fluid-sizing.git#breakpoints",
    "sass-rem": "^2.0.1",
    "svgxuse": "^1.2.6"
  }
}
